                  <Label text="From" />
                  <TextField fx:id="mDateField1" />
                  <TextField fx:id="mHourField1" />
                  <Label text="To" />
                  <TextField fx:id="mDateField2" />
                  <TextField fx:id="mHourField2" />
               </children>
            </HBox>
            <HBox>
               <children>
                  <Label text="Description" />
                  <TextField fx:id="mDescriptionField" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label text="Group" />
