         <children>
            <Text fx:id="mHeaderText" strokeType="OUTSIDE" strokeWidth="0.0" text="Appointment Dialog">
               <font>
                  <Font name="Liberation Sans Bold Italic" size="20.0" />
               </font>
            </Text>
            <HBox fx:id="mButtonBox">
               <children>
                  <Button fx:id="mApplyButton" mnemonicParsing="false" text="Apply" />
                  <Button fx:id="mCancelButton" mnemonicParsing="false" text="Cancel" />
                  <Button fx:id="mDeleteButton" mnemonicParsing="false" text="Delete" />
               </children>
            </HBox>
            <HBox fx:id="mTitleBox" prefHeight="100.0" prefWidth="200.0">
